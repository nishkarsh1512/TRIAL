      .jumbotron.container(style="background-color:#A8DDE0")
            .row
                .col-md-4
                    img.facil-img(src='./img/facilities/mill.JPG', alt='')
                .col-md-8
                    h4(style='color: #d9534f; font-family:Georgia, serif') Manual FSW Machine
                    ul.custom-bullet(style='color: #0000cc; font-family:Georgia, serif')
