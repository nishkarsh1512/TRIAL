        li.nav-item.text-center
          a#profile-tab.nav-link(data-toggle='tab', href='#bookc', role='tab', aria-controls='profile', aria-selected='false')
            | Book Chapters &emsp;
            h5(style='color: #7B1212; font-family:Georgia, serif,text.centre:align') (9)
        li.nav-item.text-center
          a#contact-tab.nav-link(data-toggle='tab', href='#ijourn', role='tab', aria-controls='contact', aria-selected='false')
            | International Journals &emsp;
            h5(style='color: #7B1212; font-family:Georgia, serif,text.centre:align') (63)
        li.nav-item.text-center
          a#contact-tab.nav-link(data-toggle='tab', href='#njourn', role='tab', aria-controls='contact', aria-selected='false')
            | National Journals &emsp;
            h5(style='color: #7B1212; font-family:Georgia, serif,text.centre:align') (1)
        li.nav-item.text-center
          a#contact-tab.nav-link(data-toggle='tab', href='#conf', role='tab', aria-controls='contact', aria-selected='false')
            | Conferences &emsp;
            h5(style='color: #7B1212; font-family:Georgia, serif,text.centre:align') (32)
